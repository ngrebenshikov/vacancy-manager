@model IEnumerable<AppTEST.Models.Profile>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            PublDate
        </th>
        <th>
            Descr
        </th>
        <th>
            userID
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.PublDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Descr)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.userID)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.profileId }) |
            @Html.ActionLink("Details", "Details", new { id=item.profileId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.profileId })
        </td>
    </tr>
}

</table>
