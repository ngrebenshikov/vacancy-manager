@model Vacan.Models.Vacancy

@{
    ViewBag.Title = "Удаление";
}

<h2>Удаление вакансии</h2>

<h3>Вы действительно хотите удалить информацию о вакансии?</h3>
<fieldset>
    <legend>Вакансия</legend>

    <div class="display-label">Название</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Name)
    </div>

    <div class="display-label">Описание</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Description)
    </div>
</fieldset>
@using (Html.BeginForm()) {
    <p>
        <input type="submit" value="Delete" /> |
        @Html.ActionLink("Back to List", "Index")
    </p>
}
